{
  "name": "@indot/fetchcontext",
  "version": "0.0.1",
  "description": "Custom fetch react hook and context provider",
  "main": "dist/fetchcontext.js",
  "scripts": {
    "test": "jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:ts && npm run gen-docs",
    "build:types": "tsc --emitDeclarationOnly",
    "build:ts": "tsc --outDir ./dist",
    "build:docs": "typedoc --readme README.md --excludeNotExported --ignoreCompilerErrors --exclude '{**/*spec*,**/node_modules/**,**/spec/**}' --out docs ./src",
    "no-jekyll": "touch docs/.nojekyll",
    "gen-docs": "npm run build:docs && npm run no-jekyll"
  },
  "keywords": [
    "React",
    "Fetch",
    "Hook",
    "React Context"
  ],
  "author": "jasmith79",
  "license": "MIT",
  "dependencies": {
    "@indot/react-ctx-store": "0.0.5",
    "@indot/usefetch": "0.0.7"
  },
  "peerDependencies": {
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/types": "^7.5.5",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.4",
    "@testing-library/react-hooks": "^2.0.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.18",
    "@types/jsdom": "^12.2.4",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "fetch-ponyfill": "^6.1.0",
    "jest": "^24.9.0",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-test-renderer": "^16.9.0",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.3"
  }
}
